{
  "name": "api",
  "private": true,
  "version": "0.0.0",
  "description": "API Part of my Movie manager projet",
  "keywords": [],
  "dependencies": {
    "axios": "^0.17.1",
    "express": "^4.15.4",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.28.6"
  },
  "scripts": {
    "debug": "node debug --experimental-modules app.mjs",
    "start": "NODE_ENV=${NODE_ENV:=production} nodemon --inspect --experimental-modules app.mjs",
    "pretest": "eslint app.mjs src/**/*.mjs; exit 0",
    "test": "NODE_ENV=test ava; exit 0",
    "test:watch": "NODE_ENV=test ava --watch"
  },
  "ava": {
    "files": [
      "tests/**/*.js",
      "!tests/_fixtures_/*"
    ],
    "sources": "src/**/*.mjs",
    "verbose": true,
    "require": [
      "@std/esm"
    ]
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com:laurentChin/movie-manager-api.git"
  },
  "author": "Laurent Janet <laurent.janet@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@std/esm": "^0.11.3",
    "ava": "^0.22.0",
    "axios-mock-adapter": "^1.10.0",
    "eslint": "^4.7.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "nodemon": "^1.12.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  }
}
