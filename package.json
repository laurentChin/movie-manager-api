{
  "name": "api",
  "private": true,
  "version": "0.3.1",
  "description": "API Part of my Movie manager projet",
  "keywords": [],
  "dependencies": {
    "apollo-server-express": "^2.3.1",
    "axios": "^0.17.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.3",
    "jsonwebtoken": "^8.3.0",
    "multer": "^1.3.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sharp": "^0.20.5",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "debug": "node debug --experimental-modules app.mjs",
    "prestart": "npm test",
    "start": "NODE_ENV=${NODE_ENV:=production} nodemon --inspect --experimental-modules public/index.mjs",
    "pretest": "eslint src/app.mjs src/**/*.mjs; exit 0",
    "test": "NODE_ENV=test ava; exit 0",
    "test:watch": "NODE_ENV=test ava --watch",
    "db:sync": "node --experimental-modules scripts/db-sync.mjs",
    "db:migrate": "node --experimental-modules scripts/db-migrate.mjs",
    "shipit": "shipit"
  },
  "ava": {
    "files": [
      "tests/**/*.mjs",
      "!tests/_fixtures_/*"
    ],
    "sources": "src/**/*.mjs",
    "verbose": true,
    "require": [
      "esm"
    ],
    "babel": false,
    "extensions": [
      "mjs"
    ],
    "node-args": [
      "--experimental-modules"
    ]
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com:laurentChin/movie-manager-api.git"
  },
  "author": "Laurent Janet <laurent.janet@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@std/esm": "^0.11.3",
    "ava": "^1.0.0-beta.5",
    "axios-mock-adapter": "^1.15.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.2",
    "prettier": "^1.16.1",
    "shipit-cli": "^4.1.1",
    "shipit-deploy": "^4.1.1",
    "shipit-shared": "^4.4.2",
    "tap-spec": "^4.1.2",
    "tape": "^4.9.1"
  }
}
